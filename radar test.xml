/><slot editor_connected="0" value="false"/><slot editor_connected="0" value="0"/></logic_slots></o></c><c d="microprocessor" t="0"><o r="1,0,0,0,1,0,0,0,1" bc="FFFFFFFF" ac="FFFFFFFF" sc="16" custom_name=""><microprocessor_definition name="Radar/Map" description="No description set." hide_in_inventory="false" width="2" length="2" id_counter="11" id_counter_node="4" transform_index="0" sym0="0" sym1="0" sym2="0" sym3="0" sym4="0" sym5="0" sym6="0" sym7="0" sym8="0" sym9="0" sym10="0" sym11="0" sym12="0" sym13="0" sym14="0" sym15="0"><nodes><n id="1" component_id="2" built_slot_index="0"><node orientation="0" label="Input Radar Ping" mode="1" type="0" description="The input signal to be processed." flags="0"><position x="0" y="0" z="0"/></node></n><n id="2" component_id="4" built_slot_index="0"><node orientation="0" label="Input Radar Bearing" mode="1" type="1" description="The input signal to be processed." flags="0"><position x="0" y="0" z="1"/></node></n><n id="3" component_id="7" built_slot_index="0"><node orientation="0" label="To Screen" mode="0" type="6" description="The input signal to be processed." flags="0"><position x="1" y="0" z="0"/></node></n><n id="4" component_id="11" built_slot_index="0"><node orientation="0" label="Input Radar Distace" mode="1" type="1" description="The input signal to be processed." flags="0"><position x="1" y="0" z="1"/></node></n></nodes><group id="0"><pos x="0" y="0"/><data type="0" name="" desc=""><inputs/><outputs/></data><components><c type="56"><object id="1" script='yaw = 0.0				-- radar yaw, relative to heading
firsttime = true 		-- true if initialisation is required
fov = 0.1				-- FOV
pointcount = 1		   -- the number of distinct data points in a complete sweep
data_d = {}  			-- distance data array
data_s = {}  			-- strength data array
data_a = {}  			-- angle data array
--range = 50.0		 	-- maximum range (m)
fade = true			  -- sets whether to fade the display or not
sweep = true			 -- set whether to show a sweep line or not
show_range = true


-- Tick function that will be executed every logic tick
function onTick()
	if firsttime then
		-- Initialise
		output.setNumber(1, 0.0) 	-- set initial yaw

		--fov = input.getNumber(4)
		fov = property.getNumber(&apos;FOV&apos;)
		output.setNumber(1, fov)
		pointcount = math.ceil(1.0/fov)

    	for i=0, pointcount do
      	data_d[i] = 0.0
      	data_s[i] = 0.0
      	data_a[i] = 0.0
	    end
	
		fade = property.getBool(&apos;Fade&apos;)
		sweep = property.getBool(&apos;Show Sweep&apos;)
		--range = property.getNumber(&apos;Range&apos;)
		show_range = property.getBool(&apos;Show Range&apos;)
	
		firsttime = false
	end
	
	
	-- Every tick
	target = input.getBool(1)
	range = input.getNumber(4)
	i = math.floor((yaw + 0.5) / fov) -- calculate current data index

	--if target then
	if true then
		-- found a target
		distance = input.getNumber(1)
		strength = input.getNumber(2)
		angle = input.getNumber(3)
		
		data_d[i] = distance
		data_s[i] = strength
		data_a[i] = angle
	
	else
		-- no target
		data_d[i] = 0.0
		data_s[i] = 0.0
		data_a[i] = 0.0
		
	end
	
	
	-- Fade the array over time
	if fade then
		for i=0, pointcount do
			data_s[i] = data_s[i] * (1.0 - (fov * 2))
		end
	end
	
	
	-- Update Yaw
	yaw = yaw + fov
	if yaw &gt; 0.5 then
		yaw = yaw - 1.0
	end
	output.setNumber(1, yaw)-- (1, yaw* -1.0)
	
end



-- Draw function that will be executed when this script renders to a screen
function onDraw()
	w = screen.getWidth()				  -- Get the screen&apos;s width and height
	h = screen.getHeight()					
	screen.setColor(0, 0, 0)
	screen.drawClear()
	hT = ((h/6)*5)-4
	hR = ((h/6)*5)+6
	-- Draw on-screen information
	if show_range then
		screen.setColor(65, 65, 80)
		screen.drawTextBox(1, hT, 35, 10, "Range", 0, 0)
		screen.setColor(65, 65, 65)
		screen.drawText(6, hR, string.format("%u", range) .. &apos;m&apos;)
	end
	

	--[[
	-- Debugging
	screen.setColor(255, 255, 255)
	screen.drawText(1, 1, &apos;FOV &apos; .. fov)
	screen.drawText(1, 7, &apos;YAW &apos; .. yaw)
	screen.drawText(1, 13, &apos;RANGE &apos; .. range)
	screen.drawText(1, 20, &apos;POINTS &apos; .. pointcount)
	
	for i=0, pointcount do
		screen.drawText(w/2, (i * 6) + 1, i .. &apos;:&apos; .. data_d[i] .. &apos;,&apos; .. data_s[i])
	end
	--]]
	-- draw the circle
	screen.setColor(10, 10, 40)--ajout cercle plein
	screen.drawCircleF(w/2, h/2, h/2) 
	screen.setColor(0, 0, 100) -- Original
	screen.drawCircle(w/2, h/2, h/2) -- Original 50m max
	screen.setColor(20, 20, 100) -- ajout de cercles ( 5 intervales )
	screen.drawCircle(w/2, h/2, h/2.51) -- 40 ou 4000 si regle a 5000m
	screen.setColor(20, 20, 100)
	screen.drawCircle(w/2, h/2, h/3.42) -- 30
	screen.setColor(20, 20, 100)
	screen.drawCircle(w/2, h/2, h/5.3) -- 20
	screen.setColor(20, 20, 100)
	screen.drawCircle(w/2, h/2, h/12) -- 10 m ou 1000 si regle a 5000m
	
	for a=0,1,0.5/w do
		i = math.floor(a/fov)
		rad = ((a * 3.14159 * 2.0)* -1) -- Ajout de "* -1"
		x = math.sin(rad) * (data_d[i] / range) * (h/2)
		y = math.cos(rad) * (data_d[i] / range) * (h/2)

		if (data_d[i] &lt;= range) and (data_s[i] &gt; 0.0) then
			-- draw the point
			c = math.ceil(data_s[i] * 255.0)
			
			screen.setColor(c, c, 0)
		else
			screen.setColor(0, 0, 0)
		end
		
		screen.drawRect(x + (w/2), y + (h/2), 1, 1)
	end
	



	-- draw the sweep line
	if sweep then
		rad = (((yaw * 3.14159 * 2.0) - 3.14159) * -1) -- Ajout de "* -1"
		x = math.sin(rad) * (h/2.05) -- (h/2)
		y = math.cos(rad) * (h/2.05)
		screen.setColor(20, 75, 20)
		screen.drawLine(x + (w/2), y + (h/2), w/2, h/2)
	end

	-- draw the centre
	screen.setColor(128, 128, 255)
	screen.drawRect(w/2,h/2,1,1)
	
	
end'><pos x="-1.25" y="0"/><in1 component_id="8" node_index="0"/><in2 component_id="0" node_index="0"/><out1/><out2/></object></c><c type="41"><object id="8" count="1" offset="0"><pos x="-2.5" y="-0.25"/><inc component_id="9" node_index="0"/><in1 component_id="2" node_index="0"/><out1/></object></c><c type="40"><object id="9" count="2" offset="0"><pos x="-2.5" y="0.5"/><inc component_id="0" node_index="0"/><in1 component_id="4" node_index="0"/><in2 component_id="11" node_index="0"/><out1/></object></c></components><components_bridge><c type="0"><object id="2"><pos x="-3.75" y="-0.25"/><in1 component_id="0" node_index="0"/><out1/></object></c><c type="2"><object id="4"><pos x="-3.75" y="1"/><in1 component_id="0" node_index="0"/><out1/></object></c><c type="7"><object id="7"><pos x="0" y="0"/><in1 component_id="1" node_index="1"/><out1/></object></c><c type="2"><object id="11"><pos x="-3.75" y="0.5"/><in1 component_id="0" node_index="0"/><out1/></object></c></components_bridge><groups/></group></microprocessor_definition><vp x="-1" y="1" z="6"/><logic_slots><slot editor_connected="0" value="false"/><slot editor_connected="0" value="0"/><slot editor_connected="0" value="0"/><slot editor_connected="0"><value/></slot></logic_slots></o></c></components></body></bodies><logic_node_links><logic_node_link type="4"><voxel_pos_0 x="-7" y="1" z="6"/><voxel_pos_1 x="5" y="5" z="6"/></logic_node_link><logic_node_link type="4"><voxel_pos_0 x="-7" y="1" z="6"/><voxel_pos_1 x="5" y="5" z="1"/></logic_node_link><logic_node_link type="4"><voxel_pos_0 x="-7" y="1" z="6"/><voxel_pos_1 x="0" y="9" z="-1"/></logic_node_link></logic_node_links></vehicle>