i=0
tB = 0
tA = {}
SUM = 0
AVG = 0


function onTick()
Beep = input.getBool(1)
Reset = input.getBool(2)

if Reset then 
	t=0
	i=0
	tB = 0
	tA = {}
	SUM = 0
	AVG = 0
end

t = t+1

if Beep then i = i+1 end

if Beep then tB = t end

tA[i] = tB


end



function onDraw()

for i=1,16,1 do
	screen.drawText(0,(i-1)*6, tA[i])
end
for i=16,32,1 do
	screen.drawText(20,(i-17)*6, tA[i])
end
for i=32,48,1 do
	screen.drawText(40,(i-33)*6, tA[i])
end


if i &gt; 48 then
SUM = 0
for i=2,48,1 do 
	SUM = SUM + (tA[i] - tA[i-1])
end
AVG = SUM/48
screen.drawText(70,20,AVG)
end


end"><pos x="0" y="0.75"/><in1 component_id="7" node_index="0"/><in2 component_id="0" node_index="0"/><out1/><out2/></object></c><c type="41"><object id="7" count="2" offset="0"><pos x="-1.5" y="0.5"/><inc component_id="0" node_index="0"/><in1 component_id="12" node_index="0"/><in2 component_id="15" node_index="0"/><out1/></object></c><c type="48"><object id="12" m="1"><pos x="-3" y="0.75"/><in1 component_id="1" node_index="0"/><out1/></object></c></components><components_bridge><c type="0"><object id="1"><pos x="-4.5" y="0.75"/><in1 component_id="0" node_index="0"/><out1/></object></c><c type="7"><object id="6"><pos x="1.5" y="0.75"/><in1 component_id="2" node_index="1"/><out1/></object></c><c type="0"><object id="15"><pos x="-3" y="0.25"/><in1 component_id="0" node_index="0"/><out1/></object></c></components_bridge><groups/></group></microprocessor_definition><vp x="-2" y="1" z="1"/><logic_slots><slot value="false"/><slot value="false"/><slot><value/></slot></logic_slots></o></c><c d="button_toggle" t="0"><o r="0,0,1,-1,0,0,0,-1,0" bc="FFFFFFFF" ac="FFFFFFFF" sc="5" custom_name="" hold_duration="1" default_state="false" sensitivity="1" val_1_name="" val_2_name=""><vp x="-2" y="1" z="5"/><logic_slots><slot value="false"/><slot value="false"/><slot value="false"/><slot value="0"/><slot value="0"/></logic_slots><min_lever_value text="0" value="0"/><max_lever_value text="1" value="1"/><starting_lever_value text="0" value="0"/></o></c><c d="01_block" t="0"><o r="-1,0,0,0,-1,0,0,0,1" bc="FFFFFFFF" ac="FFFFFFFF" sc="6"><vp x="-2" y="0" z="5"/></o></c></components></body></bodies><logic_node_links><logic_node_link type="4"><voxel_pos_0 x="0" y="1" z="3"/><voxel_pos_1 x="0" y="1" z="0"/></logic_node_link><logic_node_link type="4"><voxel_pos_0 x="0" y="1" z="0"/><voxel_pos_1 x="0" y="0" z="-2"/></logic_node_link><logic_node_link type="4"><voxel_pos_0 x="2" y="1" z="0"/><voxel_pos_1 x="0" y="1" z="0"/></logic_node_link><logic_node_link type="4"><voxel_pos_0 x="2" y="1" z="0"